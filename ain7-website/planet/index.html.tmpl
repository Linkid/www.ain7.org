<div id="hdr">
	<div id="banner"><img src="http://planet.ain7.porcheron.info/img/spacer" alt=""></div>
	<div id="logo"><a
		href="./"><img
		src="http://planet.ain7.porcheron.info/img/spacer" alt=""></a></div>

</div>


<div id="body">

<TMPL_LOOP Items>
	<TMPL_IF new_date>
		<h2 class="date"><TMPL_VAR new_date></h2>
	</TMPL_IF>

	<div class="entry<TMPL_IF channel_nick> <TMPL_VAR channel_nick></TMPL_IF>">
	
	<div class="person-info">
		<a href="<TMPL_VAR channel_link ESCAPE="HTML">" title="<TMPL_VAR channel_title ESCAPE="HTML">">
			<TMPL_IF channel_face>
				<img class="face" src="/img/heads/<TMPL_VAR channel_face ESCAPE="HTML">" alt=""><br />
			<TMPL_ELSE>
				<img class="face" src="http://planet.ain7.porcheron.info/img/heads/nobody.png" alt=""><br />
			</TMPL_IF>
			<TMPL_VAR channel_name><TMPL_IF channel_nick><br />(<TMPL_VAR channel_nick>)</TMPL_IF>
		</a>
	</div>
	
		<div class="post">
		<div class="post2">
			<div class="post-header">
				<TMPL_IF title>
				<h4 class="post-title"><a href="<TMPL_VAR link ESCAPE="HTML">"><TMPL_VAR title></a></h4>
				<TMPL_ELSE>
				<div class="post-title"><span>&nbsp;</span></div>
				</TMPL_IF>
			</div>
			<div class="post-contents">
			<TMPL_VAR content>
			</div>
			<div class="post-footer">
				<p><a href="<TMPL_VAR link ESCAPE="HTML">"><TMPL_VAR date></a></p>
			</div>
		</div>
		</div>

	</div>
</TMPL_LOOP>
</div>


